{
  "name": "vite-plugin-sentry",
  "description": "The unofficial Sentry plugin for Vite ⚡️",
  "version": "1.1.5",
  "author": "ikenfin",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "homepage": "https://github.com/ikenfin/vite-plugin-sentry",
  "repository": {
    "type": "git",
    "url": "https://github.com/ikenfin/vite-plugin-sentry"
  },
  "files": [
    "index.d.ts",
    "client.d.ts",
    "types/*",
    "dist/*"
  ],
  "keywords": [
    "vite-plugin",
    "sentry",
    "sentry-cli",
    "sourcemap"
  ],
  "scripts": {
    "bundle": "NODE_ENV=production rollup -c",
    "dev": "NODE_ENV=develop rollup -c",
    "test": "jest ./src/*/**.test.ts",
    "test:coverage": "jest --coverage ./src/*/**.test.ts",
    "lint": "eslint ./src/**/*.ts",
    "lint:tests": "eslint ./src/**/*.test.ts",
    "prepare": "is-ci || husky install"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "22.0.2",
    "@rollup/plugin-eslint": "^8.0.1",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.2",
    "eslint": "8.23.1",
    "husky": "^8.0.0",
    "is-ci": "^3.0.1",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "rollup": "2.79.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-typescript2": "0.33.0",
    "ts-jest": "^28.0.5",
    "typescript": "4.8.2"
  },
  "dependencies": {
    "@sentry/cli": "^2.3.1"
  },
  "peerDependencies": {
    "vite": "^2.6.0 || ^3.0.0"
  },
  "engines": {
    "node": ">= 12"
  }
}
